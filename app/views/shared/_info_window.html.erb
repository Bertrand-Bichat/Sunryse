<div class="container">
  <div>
    <p class="pop-title"><%= user.pseudo %></p>
    <p class="pop-time">inscrit depuis le <%= user.created_at.to_date.strftime("%d-%m-%Y") %></p>
    <p class="pop-user"><%= user.age %> ans</p>
    <%= link_to profil_path(user.slug) do %>
      <i class="fas fa-eye" title="Voir son profil"></i>
    <% end %>
  </div>
</div>
